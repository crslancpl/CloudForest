<?xml version='1.0' encoding='utf-8' ?>
<interface>
    <object class="GtkBox" id="TabButtonBox">
        <property name="orientation">vertical</property>
        <property name="spacing">10</property>
        <child>
            <object class="GtkButton" id="EditAreaButton">
                <property name="hexpand">true</property>
                <property name="label">Edit Area</property>
            </object>
        </child>
        <child>
            <object class="GtkButton" id="ExtensionButton">
                <property name="hexpand">true</property>
                <property name="label">Extensions</property>
            </object>
        </child>
        <child>
            <object class="GtkButton" id="LanguageButton">
                <property name="hexpand">true</property>
                <property name="label">Languages</property>
            </object>
        </child>
        <child>
            <object class="GtkGrid">
                <property name="vexpand">true</property>
                <property name="hexpand">true</property>
            </object>
        </child>
        <child>
            <object class="GtkButton" id="CloseButton">
                <property name="hexpand">true</property>
                <property name="label">Close</property>
            </object>
        </child>
    </object>
    <object class="GtkBox" id="EditAreaSettingPage">
        <property name="spacing">10</property>
        <property name="orientation">vertical</property>
        <child>
            <object class="GtkLabel">
                <property name="wrap">true</property>
                <property name="halign">start</property>
                <property name="name">PageName</property>
                <property name="label">Edit Area Settings</property>
            </object>
        </child>
        <child>
            <object class="GtkBox">
                <property name="spacing">10</property>
                <child>
                    <object class="GtkLabel">
                        <property name="label">Tab button behavior</property>
                    </object>
                </child>
                <child>
                    <object class="GtkDropDown" id="tabbehavechooser">
                        <property name="width-request">80</property>
                        <property name="model">
                            <object class="GtkStringList">
                                <items>
                                    <item translatable="yes">tab</item>
                                    <item translatable="yes">space</item>
                                </items>
                            </object>
                        </property>
                    </object>
                </child>
                <child>
                    <object class="GtkLabel">
                        <property name="label">Size(char width)</property>
                    </object>
                </child>
                <child>
                    <object class="GtkSpinButton" id="tabsizesetter">
              </object>
                </child>
            </object>
        </child>
        <child>
            <object class="GtkGrid">
                <property name="vexpand">true</property>
            </object>
        </child>
    </object>
    <object class="GtkBox" id="ExtensionsPage">
        <property name="spacing">10</property>
        <property name="orientation">vertical</property>
        <child>
            <object class="GtkLabel">
                <property name="wrap">true</property>
                <property name="halign">start</property>
                <property name="name">PageName</property>
                <property name="label">Extension Settings</property>
            </object>
        </child>
        <child>
            <object class="GtkGrid">
                <property name="vexpand">true</property>
            </object>
        </child>
    </object>
    <object id="SettingWindow" class="GtkWindow">
        <property name="name">SettingWindow</property>
    </object>
</interface>
